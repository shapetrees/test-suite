    { "type": "post", "label": "create /{Shared}/{Git-Users}/ericprud/",
      "path": "/{Shared}/{Git-Users}/", "slug": "ericprud", "expectedType": "Container",
      "bodyURL": "test/apps/gh-deep/ericprud-org.ttl", "root": {"@id": "#ericprud"},
      "status": 201, "parms": {"userName": "ericprud"}, "location": "/{Shared}/{Git-Users}/ericprud/",
      "find": [
        {"path": "/{Shared}/{Git-Users}/ericprud/", "accept": "text/turtle", "entries": ["nested Container for ericprud/"]}
      ],
      "dontFind": [
        {"path": "/{Shared}/{Git-Users}/ericprud-1/", "mediaType": "text/html", "entries": ["ericprud-1"]},
        {"path": "/{Shared}/{Git-Users}/ericprud/jsg/", "accept": "text/turtle", "entries": ["{Git-Users}/ericprud/jsg"]}
      ]
    },

    { "type": "post", "label": "create /{Shared}/{Git-Repos}/jsg/",
      "path": "/{Shared}/{Git-Repos}/", "slug": "jsg",
      "bodyURL": "test/apps/gh-deep/jsg.ttl", "root": {"@id": "#jsg"},
      "status": 201, "expectedType": "Container", "location": "/{Shared}/{Git-Repos}/jsg/",
      "find": [
        {"path": "/{Shared}/{Git-Repos}/jsg/", "accept": "text/turtle", "entries": ["<> a ldp:BasicContainer", "nested Container for jsg/"]}
      ],
      "dontFind": [
        {"path": "/{Shared}/{Git-Issues}/1.ttl", "accept": "text/turtle", "entries": ["{Git-Issues}/1.ttl"]}
      ]
    },

    { "type": "post", "label": "create /{Shared}/{Git-Issues}/1",
      "path": "/{Shared}/{Git-Issues}/", "slug": "1.ttl",
      "bodyURL": "test/apps/gh-deep/jsg-issue1.ttl", "root": {"@id": "#issue1"},
      "status": 201, "expectedType": "Resource", "location": "/{Shared}/{Git-Issues}/1.ttl",
      "find": [
        {"path": "/{Shared}/{Git-Issues}/1.ttl", "accept": "text/turtle", "entries": ["gh:author_association \"OWNER\""]}
      ],
      "dontFind": [
        {"path": "/{Shared}/{Git-Issues}/2.ttl", "accept": "text/turtle", "entries": ["{Git-Issues}/2.ttl"]}
      ]
    }
